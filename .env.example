# DevGuardian AI Environment Variables

# Application Keys
APP_KEY=base64:YourGeneratedAppKeyHere
AI_SERVICE_SECRET_KEY=YourAIServiceSecretKeyHere

# OAuth Credentials (Get from GitHub/GitLab/Bitbucket)
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITLAB_CLIENT_ID=your_gitlab_client_id
GITLAB_CLIENT_SECRET=your_gitlab_client_secret
BITBUCKET_CLIENT_ID=your_bitbucket_client_id
BITBUCKET_CLIENT_SECRET=your_bitbucket_client_secret

# AI Service API Keys
OPENAI_API_KEY=your_openai_api_key
HUGGINGFACE_API_KEY=your_huggingface_api_key

# Database (for local development, use docker-compose)
DB_HOST=localhost
DB_PORT=5432
DB_DATABASE=devguardian_ai
DB_USERNAME=devguardian
DB_PASSWORD=secure_password_123

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Service URLs
APP_URL=http://localhost:8001
AI_SERVICE_URL=http://localhost:8000

# Frontend
VITE_API_URL=http://localhost:8001
VITE_AI_SERVICE_URL=http://localhost:8000
